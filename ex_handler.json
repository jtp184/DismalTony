{
  "handler_name": "schedule-assign",
  "verbs": [
    "move",
    "switch",
    "put"
  ],
  "patterns": [
    "move\\s(?<staff_member>(\\w+ ?)*)\\sto\\s(?<day>\\w+)\\s(?<event_key>\\w+)",
    "switch\\s(?<staff_member>(\\w+ ?)*)\\sto\\s(?<day>\\w+)\\s(?<event_key>\\w+)",
    "put\\s(?<staff_member>(\\w+ ?)*)\\son\\s(?<day>\\w+)\\s(?<event_key>\\w+)"
  ],
  "pattern_examples": [
    "Move Peeps to Friday Breakfast",
    "Switch Spyro to Thursday Group 3 Activities",
    "Put Hype! on Sunday Gatekeeper"
  ],
  "data": [
    {
      "key": "staff_member",
      "type": "string"
    },
    {
      "key": "day",
      "type": "string"
    },
    {
      "key": "event_key",
      "type": "string"
    }
  ]
}